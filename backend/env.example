# Configurações do Servidor
NODE_ENV=development
PORT=3001
WEB_ORIGIN=http://localhost:3000

# Configurações do Banco de Dados
DATABASE_URL="mysql://user:password@localhost:3306/vb_solution"

# Configurações de Log
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Configurações de Upload
UPLOAD_DIR=uploads
MAX_FILE_SIZE=26214400

# Configurações do WhatsApp (Venom-Bot)
VENOM_SESSION_DIR=sessions
VENOM_MULTI_DEVICE=true
VENOM_HEADLESS=true
VENOM_USE_CHROME=true

# Token de Autenticação (Desenvolvimento)
VB_DEV_TOKEN=vb_dev_token_12345

# Configurações de Segurança
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
